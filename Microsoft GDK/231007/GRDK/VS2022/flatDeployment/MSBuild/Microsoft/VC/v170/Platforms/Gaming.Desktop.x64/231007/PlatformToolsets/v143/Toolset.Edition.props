<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <Import Project="$(XdkEditionRootVS2022)$(XdkEdition)\PlatformToolsets\v143\ImportBefore\*.props"
            Condition="Exists('$(XdkEditionRootVS2022)$(XdkEdition)\PlatformToolsets\v143\ImportBefore')" />

    <Import Project="$(VCTargetsPath)\Platforms\x64\PlatformToolsets\v143\toolset.props"
            Condition="Exists('$(VCTargetsPath)\Platforms\x64\PlatformToolsets\v143\toolset.props')" />

    <Import Project="$(XdkEditionRootVS2022)$(XdkEdition)\Platform.Common.props" />

    <Import Project="$(XdkEditionRootVS2022)$(XdkEdition)\PlatformToolsets\v143\ImportAfter\*.props"
            Condition="Exists('$(XdkEditionRootVS2022)$(XdkEdition)\PlatformToolsets\v143\ImportAfter')" />
</Project>
